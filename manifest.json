{
    "manifest_version": 3,
    "name": "jielong.co",
    "version": "1.1",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "64": "icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "64": "icon.png"
        }
    },
    "options_page": "popup.html",
    "host_permissions": ["*://*.jielong.co/"],
    "permissions": ["tabs", "cookies", "storage", "notifications", "alarms"]
}
